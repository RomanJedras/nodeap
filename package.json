{
  "name": "file-upload",
  "version": "1.0.0",
  "description": "First project",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "npm run test:html",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build:sass  build:autoprefixer -p watch:*",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css",
    "watch:sassprefixer": "npm-run-all watch:* ",
    "stylelint": "stylelint '**/*.scss'",
    "browsersync": "browser-sync start --server --files css/*.css *.html",
    "scss": "node-sass  --output-style compact  -watch  -o css sass",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}' "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "all": "0.0.0",
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "font-awesome": "^4.7.0",
    "globstar": "^1.0.0",
    "html-validator-cli": "^4.1.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "github:stylelint/stylelint",
    "stylelint-config-standard": "file:node_modules/stylelint-config-standard",
    "stylelint-prettier": "^1.0.6"
  },
  "dependencies": {
    "@primer/css": "github:primer/css",
    "ansi-regex": "file:node_modules/ansi-regex",
    "ansi-styles": "file:node_modules/ansi-styles",
    "colors": "^1.3.3",
    "formidable": "^1.2.1",
    "normalize.css": "^8.0.1",
    "primer": "^11.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "save-dev": "^2.0.0",
    "stylelint-config-primer": "^5.0.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-scss": "^3.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomanJedras/dashborad-project.git"
  },
  "bugs": {
    "url": "https://github.com/RomanJedras/dashborad-project/issues"
  },
  "homepage": "https://github.com/RomanJedras/dashborad-project#readme"
}
